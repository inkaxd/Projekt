	{% javascripts 
	'@ProjektWypozyczalniaSamochodow/Resources/public/assets/hover-dropdown/bootstrap-hover-dropdown.min.js' 
	'@ProjektWypozyczalniaSamochodow/Resources/public/assets/page-scroller/jquery.ui.totop.min.js' 
