	'@ProjektWypozyczalniaSamochodow/Resources/public/assets/bootstrap/js/tooltip.js' 
	'@ProjektWypozyczalniaSamochodow/Resources/public/assets/bootstrap/js/popover.js' 
	'@ProjektWypozyczalniaSamochodow/Resources/public/js/wp.app.js'
	'@ProjektWypozyczalniaSamochodow/Resources/public/js/data.js'
	filter='cssrewrite' %}
	 <script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}
	
	
